<h1>Contents</h1>

<% @contents.each_key do |page| %>  
  <% @contents[page].each_key do |name| %>  
    <h2><%= page %> :  <%= name %></h2>
    <% @contents[page][name].each_key do |locale| %>  
      <p><%= locale %> <%= link_to "<i class='icon-edit'></i>".html_safe, edit_cmsino_content_path(@contents[page][name][locale]) %></p>
      <p>
      title: <%= @contents[page][name][locale][:title] %> <br/> 
      <%= @contents[page][name][locale][:text] %>  
      </p>
    <% end %>
  <% end %>
<% end %>
